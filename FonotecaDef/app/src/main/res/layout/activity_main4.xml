<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main4Activity">

    <TextView
            android:text="Lista de Resultados"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textColor="@android:color/background_dark" android:textSize="25sp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="63dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="84dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="84dp"/>
    <Switch
            android:text="Cancion 1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/switch1" android:layout_marginTop="25dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="74dp"/>
    <Switch
            android:text="Cancion 2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/switch2" android:layout_marginTop="31dp"
            app:layout_constraintTop_toBottomOf="@+id/switch1" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="74dp"/>
    <Switch
            android:text="Cancion 3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/switch3" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/switch2" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="74dp"/>
</android.support.constraint.ConstraintLayout>